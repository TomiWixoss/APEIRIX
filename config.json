{
	"$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.7.json",
	"author": "TomiSakae",
	"name": "APEIRIX",
	"packs": {
		"behaviorPack": "./addon-generator/build/BP",
		"resourcePack": "./addon-generator/build/RP"
	},
	"regolith": {
		"dataPath": "./packs/data",
		"filterDefinitions": {
			"typescript_build": {
				"runWith": "shell",
				"command": "powershell -ExecutionPolicy Bypass -File ../../build-scripts.ps1"
			}
		},
		"formatVersion": "1.7.0",
		"profiles": {
			"default": {
				"export": {
					"build": "standard",
					"readOnly": false,
					"target": "development"
				},
				"filters": [
					{
						"filter": "typescript_build"
					}
				]
			},
			"release": {
				"export": {
					"build": "standard",
					"readOnly": false,
					"target": "local"
				},
				"filters": [
					{
						"filter": "typescript_build"
					}
				]
			}
		}
	}
}